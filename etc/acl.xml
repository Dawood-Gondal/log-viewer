<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::system">
                    <resource id="M2Commerce_LogViewer::log_viewer" title="M2Commerce Log Viewer" sortOrder="999">
                        <resource id="M2Commerce_LogViewer::log_viewer_settings" title="Log Viewer Settings" sortOrder="10"/>
                        <resource id="M2Commerce_LogViewer::log_viewer_view" title="View Logs" sortOrder="20"/>
                        <resource id="M2Commerce_LogViewer::log_viewer_download" title="Download Logs" sortOrder="30"/>
                        <resource id="M2Commerce_LogViewer::log_viewer_delete" title="Delete Logs" sortOrder="40"/>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>

